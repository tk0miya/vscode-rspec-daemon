{
  "name": "rspec-daemon",
  "displayName": "rspec-daemon",
  "description": "VSCode extension to run rspec testing quickly with rspec-daemon",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rspec-daemon.invokeRSpecDaemon",
        "title": "RSpec Daemon: Invoke rspec-daemon"
      },
      {
        "command": "rspec-daemon.startRSpecForCurrentFile",
        "title": "RSpec Daemon: Run rspec for current file"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.78.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4",
    "@vscode/test-electron": "^2.3.0"
  }
}
